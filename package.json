{
  "name": "reactnativeboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "audit": "yarn audit",
    "start": "react-native start",
    "start-reset-cache": "react-native start --reset-cache",
    "yarn-i": "yarn install",
    "pod-install": "cd ios && pod install && cd ..",
    "jetify": "npx jetify",
    "setup": "yarn run yarn-i && yarn run jetify && yarn run pod-install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "setup-env": "./scripts/envscript.sh",
    "lint": "yarn eslint './app/**/*.{js,jsx,ts,tsx}'",
    "eslint-fix": "yarn eslint --fix './app/**/*.{js,jsx,ts,tsx}'",
    "pretty": "yarn prettier --write './app/**/*.{js,jsx,ts,tsx}'",
    "types": "yarn tsc --noEmit './app/**/*.{js,jsx,ts,tsx}'",
    "spelling": "yarn cspell lint './app/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "postinstall": "chmod +x ./scripts/envscript.sh",
    "outdated": "yarn outdated",
    "all-package-update": "yarn upgrade --latest"
  },
  "dependencies": {
    "@manaflair/redux-batch": "^1.0.0",
    "@react-native-clipboard/clipboard": "^1.11.0",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-navigation/core": "^6.3.0",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/routers": "^6.1.2",
    "@react-navigation/stack": "^6.2.3",
    "@reduxjs/toolkit": "^1.8.5",
    "@sentry/react-native": "^4.3.1",
    "apisauce": "^2.1.6",
    "appcenter": "^4.4.4",
    "class-transformer": "^0.5.1",
    "dayjs": "^1.11.5",
    "formik": "^2.2.9",
    "i18next": "^21.9.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-native": "^0.70.1",
    "react-native-code-push": "^7.0.5",
    "react-native-dotenv": "^3.3.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.6.1",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-haptic-feedback": "^1.14.0",
    "react-native-image-crop-picker": "^0.38.0",
    "react-native-localize": "^2.2.3",
    "react-native-mmkv-storage": "^0.8.0",
    "react-native-modal": "^13.0.1",
    "react-native-offline": "^6.0.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-permissions": "^3.6.1",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-spinner-button": "^1.0.9",
    "react-native-splash-screen": "https://github.com/alex-a-pereira/react-native-splash-screen.git#fix/ios-show-and-refactor",
    "react-native-svg": "^13.1.0",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rn-custom-style-sheet": "^3.1.0",
    "socket.io-client": "^4.5.2",
    "tinycolor2": "^1.4.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tsconfig/react-native": "^2.0.2",
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-decorators": "^7.19.1",
    "@babel/runtime": "^7.19.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "@react-navigation/devtools": "^6.0.9",
    "@types/i18n-js": "^3.8.3",
    "@types/jest": "^29.0.2",
    "@types/react": "^18.0.20",
    "@types/react-native": "^0.70.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.0.3",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cspell": "^6.9.1",
    "eslint": "^8.23.1",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "metro-react-native-babel-preset": "^0.72.3",
    "prettier": "^2.7.1",
    "react-native-flipper": "^0.164.0",
    "react-test-renderer": "^18.2.0",
    "redux-flipper": "^2.0.2",
    "typescript": "^4.8.3"
  },
  "resolutions": {
    "@types/react": "^18.0.15"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}