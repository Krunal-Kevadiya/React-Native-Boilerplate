{
  "name": "{{project_name_with_lower_case}}",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "audit": "yarn audit",
    "start": "react-native start",
    "start-reset-cache": "react-native start --reset-cache",
    "yarn-i": "yarn install",
    "pod-install": "cd ios && pod install && cd ..",
    "jetify": "npx jetify",
    "setup": "yarn run yarn-i && yarn run jetify && yarn run pod-install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:release": "yarn lint && react-native run-android --variant=release",
    "ios:release": "yarn lint && react-native run-ios --configuration Release",
    {% if 'EsLint' in feature -%}
    "lint": "eslint './app/**/*.{js,jsx,ts,tsx}'",
    "eslint-fix": "eslint --fix './app/**/*.{js,jsx,ts,tsx}'",
    {% endif -%}
    {% if 'Prettier' in feature -%}
    "pretty": "prettier --write './app/**/*.{js,jsx,ts,tsx}'",
    {% endif -%}
    "types": "tsc --noEmit",
    {% if 'CSpell' in feature -%}
    "spelling": "cspell lint './app/**/*.{js,jsx,ts,tsx}'",
    {% endif -%}
    "outdated": "yarn outdated",
    "all-package-update": "yarn upgrade --latest"
    {%- if react_native_web -%}
    ,
    "web": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack",
    "run-server": "cross-env NODE_ENV=production nodemon server.js",
    "start-server": "yarn build && yarn run-server"
  {% endif -%}
  },
  "dependencies": {
    "@manaflair/redux-batch": "^1.0.0",
    "@react-native-clipboard/clipboard": "^1.13.2",
    "@react-navigation/core": "^6.4.11",
    "@react-navigation/native": "^6.1.12",
    "@react-navigation/routers": "^6.1.9",
    "@react-navigation/stack": "^6.3.23",
    {% if state_management == 'React Redux' -%}
    "@reduxjs/toolkit": "^2.2.1",
    {% endif -%}
    {% if 'Sentry' in feature -%}
    "@sentry/react-native": "^5.19.1",
    {% endif -%}
    {% if 'CodePush' in feature -%}
    "appcenter": "^5.0.1",
    {% endif -%}
    {% if 'AWS Amplify' in api_middleware -%}
    "aws-amplify": "^6.0.18",
    {% endif -%}
    {% if 'Axios' in api_middleware -%}
    "axios": "^1.6.7",
    {% endif -%}
    "class-transformer": "^0.5.1",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    {% if 'Translations' in feature -%}
    "i18next": "^23.10.0",
    {% endif -%}
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    {% if react_native_web -%}
    "react-dom": "^18.2.0",
    {% endif -%}
    {% if 'Translations' in feature -%}
    "react-i18next": "^14.0.5",
    {% endif -%}
    "react-native": "^0.73.5",
    {% if 'CodePush' in feature -%}
    "react-native-code-push": "^8.2.1",
    {% endif -%}
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.13.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-image-crop-picker": "^0.40.3",
    {% if 'Translations' in feature -%}
    "react-native-localize": "^3.0.6",
    {% endif -%}
    "react-native-mmkv": "^2.12.1",
    "react-native-modal": "^13.0.1",
    {% if state_management == 'React Redux' -%}
    "react-native-offline": "^6.0.2",
    {% endif -%}
    "react-native-orientation-locker": "^1.6.0",
    "react-native-permissions": "^4.1.4",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "^3.7.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-spinner-button": "^1.4.0",
    "react-native-splash-screen": "https://github.com/alex-a-pereira/react-native-splash-screen.git#fix/ios-show-and-refactor",
    "react-native-svg": "^15.0.0",
    {%- if react_native_web -%}
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.19.10",
    {% endif -%}
    {% if state_management == 'React Redux' -%}
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    {% endif -%}
    {% if state_management_middleware == 'Redux Saga' -%}
    "redux-saga": "^1.3.0",
    {% endif -%}
    {% if state_management == 'GraphQL' -%}
    "@apollo/client": "^3.9.5",
    "apollo3-cache-persist": "^0.14.1",
    "graphql": "^16.8.1",
    {% endif -%}
    "reflect-metadata": "^0.2.1",
    "rn-custom-style-sheet": "^4.0.1",
    {%- if react_native_web -%}
    "sass": "^1.71.1",
    {% endif -%}
    {% if 'Socket' in feature -%}
    "socket.io-client": "^4.7.4",
    {% endif -%}
    "tinycolor2": "^1.6.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    {% if react_native_web -%}
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    {% endif -%}
    "@babel/preset-env": "^7.20.0",
    {% if react_native_web -%}
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    {% endif -%}
    "@babel/runtime": "^7.20.0",
    "@jest/globals": "^29.7.0",
    "@react-native/babel-preset": "^0.73.21",
    {% if 'EsLint' in feature -%}
    "@react-native/eslint-config": "^0.73.2",
    {% endif -%}
    "@react-native/metro-config": "^0.73.5",
    "@react-native/typescript-config": "^0.73.1",
    {% if react_native_web -%}
    "@svgr/webpack": "^8.1.0",
    {% endif -%}
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/tinycolor2": "^1.4.6",
    "babel-jest": "^29.6.3",
    {% if react_native_web -%}
    "babel-loader": "^9.1.3",
    "babel-plugin-react-native-web": "^0.19.9",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^10.0.0",
    "connect-history-api-fallback": "^2.0.0",
    "cross-env": "^7.0.3",
    {% endif -%}
    {% if 'CSpell' in feature -%}
    "cspell": "^8.2.3",
    {% endif -%}
    {% if react_native_web -%}
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    {% endif -%}
    {% if 'EsLint' in feature -%}
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    {% endif -%}
    {% if react_native_web -%}
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    {% endif -%}
    "jest": "^29.6.3",
    {% if 'LeftHook' in feature -%}
    "lefthook": "^1.5.5",
    "lint-staged": "^15.2.0",
    {% endif -%}
    {% if react_native_web -%}
    "metro-react-native-babel-preset": "^0.77.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "path-browserify": "^1.0.1",
    {% endif -%}
    {% if 'Prettier' in feature -%}
    "prettier": "^2.8.8",
    {% endif -%}
    "react-test-renderer": "^18.2.0"
    {%- if react_native_web -%}
    ,
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "typescript": "5.0.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  {% endif -%}
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {},
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
