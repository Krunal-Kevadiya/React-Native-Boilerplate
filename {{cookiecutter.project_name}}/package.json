    "react-native-svg": "^13.1.0",
    {% if cookiecutter.state_management != 'graphql' -%}
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    {% if cookiecutter.state_management == 'saga' -%}
    "redux-saga": "^1.2.2",
    {% endif -%}
    {% endif -%}
    {% if cookiecutter.state_management == 'graphql' -%}
    "@apollo/client": "^3.7.2",
    "apollo3-cache-persist": "^0.14.1",
    "graphql": "^16.6.0",
    {% endif -%}
    "reflect-metadata": "^0.1.13",
