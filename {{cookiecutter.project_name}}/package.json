    "@react-navigation/routers": "^6.1.6",
    "@react-navigation/stack": "^6.3.9",
    "@reduxjs/toolkit": "^1.9.1",
    "@sentry/react-native": "^4.12.0",
    "apisauce": "^2.1.6",
    "axios": "^1.2.1",
    "appcenter": "^4.4.4",
    "class-transformer": "^0.5.1",
